{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach/Debug Docker C Server",
      "type": "cppdbg",
      "request": "attach",
      "program": "/srv/build/server",
      // "processId": "${command:pickProcess}",
      "args": [],
      "stopAtEntry": true,
      "cwd": "/srv",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "/usr/bin/gdb",
      "miDebuggerArgs": "",
      "logging": {
        "engineLogging": false
      },
      "pipeTransport": {
        "pipeCwd": "${workspaceRoot}",
        "pipeProgram": "docker.exe",
        "pipeArgs": ["exec", "-i", "venora-server-1", "sh", "-c"],
        "debuggerPath": "/usr/bin/gdb"
      },
      "sourceFileMap": {
        "/srv": "${workspaceFolder}/server"
      },
      "env": {
        "DATABASE_USER": "TESTUSER",
        "DATABASE_PASSWORD": "/run/secrets/db-password"
      }
    },
    // "pipeArgs": [
    //           "exec",
    //           "-i",
    //           "venora-server-1",
    //           "gdbserver",
    //           "localhost:9394",
    //           "/srv/build/server"
    //         ],

    // {
    //   "name": "Docker C Server Debug",
    //   "type": "docker",
    //   "request": "launch",
    //   "preLaunchTask": "docker-run: debug"
    //   // "platform": "C",
    // },
    {
      "name": "Python Debugger: Module",
      "type": "debugpy",
      "request": "launch",
      "module": "venora-client"
    },
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/server/build/server",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "type": "cmake",
      "request": "launch",
      "name": "CMake: Configure project",
      "cmakeDebugType": "configure",
      "clean": false,
      "configureAll": false
    }
  ]
}
